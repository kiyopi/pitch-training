{
  "name": "pitch-training-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev --turbopack -H 0.0.0.0",
    "dev:safe": "NODE_ENV=development next dev --port 3001",
    "dev:debug": "NODE_ENV=development next dev --turbopack --experimental-debug",
    "dev:simple": "NODE_ENV=development node simple-server.js",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "github-pages": "npm run build && touch out/.nojekyll",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next && rm -rf out",
    "port-check": "lsof -i :3000",
    "kill-port": "npx kill-port 3000"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "next": "15.4.1",
    "pitchy": "^4.1.0",
    "react": "19.1.0",
    "react-audio-visualize": "^1.2.0",
    "react-dom": "19.1.0",
    "react-share": "^5.2.2",
    "tailwind-merge": "^3.3.1",
    "tone": "^15.1.22"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
